<div class="body-container">
    <h2 class="head-topic">登録施設一覧</h2>
    <table class="table align-middle">
        <thead class="">
            <tr>
                <th>画像</th>
                <th>施設名</th>
                <th>紹介文</th>
                <th>料金</th>
                <th>作成日</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
        <% @rooms.each do |room| %>
            <tr>
                <td><%= room.name %></td>
                <td><%= room.detail %></td>
                <td><%= room.price %></td>
                <td><%= room.created_at %></td>
            </tr>
        <% end %>
        </tbody>
    </table>
</div>